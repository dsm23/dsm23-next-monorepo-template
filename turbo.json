{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "inputs": ["$TURBO_DEFAULT$", ".env*", "!**/*.stories.{tsx,jsx,mdx}"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "build:storybook": {},
    "coverage": {
      "outputs": ["coverage/**"]
    },
    "lint": {},
    "lint:css": {},
    "lint:lint-staged": {
      "dependsOn": ["^lint"]
    },
    "check-types": {},
    "e2e": {
      "outputs": ["test-results/**"]
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "transit": {
      "dependsOn": ["^transit"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
